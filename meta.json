{
  "module_id": "viam:ultrasonic",
  "visibility": "public",
  "url": "https://github.com/viam-modules/viam-ultrasonic",
  "description": "Viam Go module for ultrasonic sensor and camera",
  "models": [
    {
      "api": "rdk:component:sensor",
      "model": "viam:ultrasonic:sensor",
      "markdown_link" : "README.md#configure-your-ultrasonic-sensor",
      "short_description" : "driver to return readings from an ultrasonic sensor with trigger and echo pins"

    },
    {
      "api": "rdk:component:camera",
      "model": "viam:ultrasonic:camera",
      "markdown_link" : "README.md#configure-your-ultrasonic-camera",
      "short_description" : "driver to return pointclouds with one dimension from an ultrasonic sensor with trigger and echo pins"
    }
  ],
  "build": {
    "build": "make module.tar.gz",
    "path": "module.tar.gz",
    "arch" : ["linux/arm64"]
  },
  "entrypoint": "bin/ultrasonic-module"
}
